<h1>Lists#show</h1>
<%= link_to "Add a movie", new_list_bookmark_path(@list), class: "btn btn-warning" %>
<% @list.bookmarks.each do |bookmark| %>
<p><%= bookmark.movie.title %>
<%= link_to"Delete it",bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}%>
</p>
<% end %>
<%= link_to "Back to home", lists_path, class: "btn btn-warning" %>
